<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
    <bean id="cogHttpExtension" class="org.geoserver.platform.ModuleStatusImpl">
        <constructor-arg index="0" value="gs-cog-http"/>
        <constructor-arg index="1" value="COG HTTP Extension"/>
        <property name="message" value="COG module supporting simple HTTP range read requests" />
        <property name="category" value="COMMUNITY"/>
    </bean>

</beans>
