<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

	<bean id="csvStorePanel" class="org.geoserver.web.data.resource.DataStorePanelInfo">
		<property name="id" value="csv"/>
		<property name="factoryClass" value="org.geotools.data.csv.CSVDataStoreFactory"/>
		<property name="iconBase" value="org.geoserver.web.GeoServerApplication" />
		<property name="icon" value="img/icons/geosilk/page_white_vector.png" />
		<property name="componentClass"
				  value="org.geoserver.web.data.store.csv.CSVStoreEditPanel" />
	</bean>

</beans>
