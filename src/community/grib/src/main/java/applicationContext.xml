<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

  <bean id="gribStorePanel" class="org.geoserver.web.data.resource.DataStorePanelInfo">
    <property name="id" value="gribRaster" />
    <property name="factoryClass" value="org.geotools.coverage.io.grib.GRIBFormat" />
    <property name="iconBase" value="org.geoserver.web.GeoServerApplication" />
    <property name="icon" value="img/icons/geosilk/page_white_raster.png" />
    <property name="componentClass"
      value="org.geoserver.web.data.store.raster.GribRasterEditPanel" />
  </bean>
 
</beans>
