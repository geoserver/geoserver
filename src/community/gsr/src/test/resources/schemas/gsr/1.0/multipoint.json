{
    "$schema":"https://json-schema.org/draft/2020-12/schema",

    "title":"multi point",
    "type":"object",
    "extends":{
        "$ref":"geometry.json"
    },
    "properties":{
        "points":{
            "title":"points",
            "type":"array",
            "items":{
                "title":"direct position [x,y]",
                 "type":"array",
                 "items":{
                     "title":"ordinate",
                     "type":"number"
                 }
             }
         },
        "spatialReference":{
            "$ref" : "spatialreference.json"}
    },
    "required":["points"]
}
