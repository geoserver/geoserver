{
   "$schema":"https://json-schema.org/draft/2020-12/schema",

    "title":"range domain",
    "type":"object",
    "extends":{
        "$ref":"domain.json"
    },
    "properties":{
        "type":{
            "title":"domain type",
            "type":"string",
            "enum":["range"]
        },
        "name":{
            "title":"domain name",
            "type":"string"
        },
        "range":{
            "title":"range (min, max)",
            "type":"array",
            "minItems":2,
            "maxItems":2,
            "items":{
                "type":"number"
            }
        }
    },
    "required":["type","name","range"]
}
