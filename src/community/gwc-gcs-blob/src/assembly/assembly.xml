                <include>gson*.jar</include>
                <include>guava*.jar</include>
                <include>threetenbp*.jar</include>
                <include>conscrypt*.jar</include>
                <include>perfmark*.jar</include>
                <include>annotations*.jar</include>
                <include>checker-qual*.jar</include>
