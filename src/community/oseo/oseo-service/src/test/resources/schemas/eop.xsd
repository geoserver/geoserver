					<documentation>Other, or unspecified orientation.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="CW">
				<annotation>
					<documentation>Clockwise</documentation>
				</annotation>
			</enumeration>
			<enumeration value="CCW">
				<annotation>
					<documentation>CounterClockwise</documentation>
				</annotation>
			</enumeration>
