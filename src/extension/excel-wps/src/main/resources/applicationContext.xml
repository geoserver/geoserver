<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="WPSExcelExtension" class="org.geoserver.platform.ModuleStatusImpl">
        <property name="module" value="gs-excel-wps" />
        <property name="name" value="Excel WPS download output extension"/>
        <property name="component" value="Excel WPS download output extension"/>
        <property name="available" value="true"/>
        <property name="enabled" value="true"/>
        <property name="category" value="EXTENSION"/>
    </bean>
    <bean id="ExcelPPIO" class="org.geoserver.wps.ppio.XLSXPPIO"/>
</beans>
