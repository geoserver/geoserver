<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

  <bean id="JP2KExtension"
		class="org.geoserver.platform.ModuleStatusImpl">
		<property name="module" value="gs-jp2k" />
		<property name="name" value="JP2K Extension" />
		<property name="component" value="JP2K plugin" />
