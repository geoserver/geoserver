<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
  <bean id="queryLayerFunctionsInitializer"
    class="org.geoserver.filter.function.QueryFunctionFactoryInitializer" />
    <bean id="QueryLayerExtension"
		class="org.geoserver.platform.ModuleStatusImpl">
		<property name="module" value="gs-querylayer" />
		<property name="name" value="Query Layer Extension" />
		<property name="component" value="Query Layer plugin" />
		<property name="available" value="true" />
		<property name="enabled" value="true" />
        <property name="category" value="EXTENSION"/>
	</bean>
</beans>
